    {
      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "\n",
        "# Hypothetical Data\n",
        "data = {\n",
        "    'Literary Work': ['A', 'A', 'B', 'B', 'C', 'C'],\n",
        "    'Translator Type': ['Human', 'Machine', 'Human', 'Machine', 'Human', 'Machine'],\n",
        "    'CNR': [85, 65, 78, 58, 82, 60],\n",
        "    'LA': [92, 85, 89, 81, 90, 83],\n",
        "    'EI': [88, 70, 84, 65, 87, 72],\n",
        "    'RC': [90, 80, 87, 77, 88, 78]\n",
        "}\n",
        "\n",
        "# Convert data to DataFrame\n",
        "df = pd.DataFrame(data)\n",
        "\n",
        "# Group by Translator Type and calculate mean scores per category\n",
        "category_columns = ['CNR', 'LA', 'EI', 'RC']\n",
        "df_grouped = df.groupby('Translator Type')[category_columns].mean()\n",
        "\n",
        "# Plotting the Bar Graph\n",
        "bar_width = 0.35\n",
        "index = np.arange(len(category_columns))\n",
        "\n",
        "fig, ax = plt.subplots(figsize=(10, 6))\n",
        "\n",
        "# Bar positions\n",
        "bar1 = ax.bar(index, df_grouped.loc['Human'], bar_width, label='Human', color='red')\n",
        "bar2 = ax.bar(index + bar_width, df_grouped.loc['Machine'], bar_width, label='Machine', color='blue')\n",
        "\n",
        "# Adding labels and title\n",
        "ax.set_xlabel('Category', fontsize=12)\n",
        "ax.set_ylabel('Average Score', fontsize=12)\n",
        "ax.set_title('A Comparison of The Effectiveness of MT and Human Translations', fontsize=14)\n",
        "ax.set_xticks(index + bar_width / 2)\n",
        "ax.set_xticklabels(category_columns, fontsize=12)\n",
        "ax.legend()\n",
        "\n",
        "# Show plot\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ],
      "metadata": {
