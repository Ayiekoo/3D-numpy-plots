      "source": [
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "import pandas as pd\n",
        "\n",
        "# Hypothetical Data\n",
        "data = {\n",
        "    'Literary Work': ['A', 'A', 'B', 'B', 'C', 'C'],\n",
        "    'Translator Type': ['Human', 'Machine', 'Human', 'Machine', 'Human', 'Machine'],\n",
        "    'CNR': [85, 65, 78, 58, 82, 60],\n",
        "    'LA': [92, 85, 89, 81, 90, 83],\n",
        "    'EI': [88, 70, 84, 65, 87, 72],\n",
        "    'RC': [90, 80, 87, 77, 88, 78]\n",
        "}\n",
        "\n",
        "# Convert data to DataFrame\n",
        "df = pd.DataFrame(data)\n",
        "\n",
        "# Reshape the DataFrame for seaborn (long format)\n",
        "df_melted = df.melt(id_vars=['Literary Work', 'Translator Type'], value_vars=['CNR', 'LA', 'EI', 'RC'],\n",
        "                    var_name='Category', value_name='Score')\n",
        "\n",
        "# Create the Boxplot\n",
        "plt.figure(figsize=(10, 6))\n",
        "sns.boxplot(x='Category', y='Score', hue='Translator Type', data=df_melted, palette='Set2')\n",
        "\n",
        "# Add labels and title\n",
        "plt.title('A Comparison of The Effectiveness of MT and Human Translations', fontsize=14)\n",
        "plt.xlabel('Category', fontsize=12)\n",
        "plt.ylabel('Score', fontsize=12)\n",
        "plt.legend(title='Translator Type')\n",
        "\n",
        "# Show plot\n",
        "plt.show()\n"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "\n",
        "# Data\n",
        "literary_works = ['A', 'B', 'C']\n",
        "categories = ['CNR', 'LA', 'EI', 'RC']\n",
        "\n",
        "# Scores for each translator type and work\n",
        "human_scores = [\n",
        "    [85, 92, 88, 90],  # Work A\n",
        "    [78, 89, 84, 87],  # Work B\n",
        "    [82, 90, 87, 88],  # Work C\n",
        "]\n",
        "machine_scores = [\n",
        "    [65, 85, 70, 80],  # Work A\n",
        "    [58, 81, 65, 77],  # Work B\n",
        "    [60, 83, 72, 78],  # Work C\n",
        "]\n",
        "\n",
        "# Plotting\n",
        "bar_width = 0.35\n",
        "index = np.arange(len(categories))\n",
        "\n",
        "fig, ax = plt.subplots(figsize=(12, 8))\n",
        "\n",
        "for i, work in enumerate(literary_works):\n",
        "    # Shift bars for each work\n",
        "    human_position = index + i * 2 * bar_width\n",
        "    machine_position = human_position + bar_width\n",
        "\n",
        "    # Create bars\n",
        "    ax.bar(\n",
        "        human_position,\n",
        "        human_scores[i],\n",
        "        bar_width,\n",
        "        label=f'Human - Work {work}',\n",
        "        color='skyblue'\n",
        "    )\n",
        "    ax.bar(\n",
        "        machine_position,\n",
        "        machine_scores[i],\n",
        "        bar_width,\n",
        "        label=f'Machine - Work {work}',\n",
        "        color='lightcoral'\n",
        "    )\n",
        "\n",
        "# Adding labels and title\n",
        "ax.set_xlabel('Categories', fontsize=12)\n",
        "ax.set_ylabel('Scores (%)', fontsize=12)\n",
        "ax.set_title('Comparative Bar Graph: Human vs Machine Translations by Literary Work', fontsize=14)\n",
        "ax.set_xticks(index + (len(literary_works) - 1) * bar_width / 2)\n",
        "ax.set_xticklabels(categories, fontsize=12)\n",
        "ax.legend()\n",
        "\n",
        "# Show plot\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ],
      "metadata": {
